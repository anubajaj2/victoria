<core:FragmentDefinition xmlns="sap.ui.table" xmlns:m="sap.m" xmlns:core="sap.ui.core">
			<m:Page showHeader="false" class="sapUiContentPadding" enableScrolling="false">
				<m:content>
				<Table
				rows=""
				title=""
				selectionMode="MultiToggle"
				visibleRowCount="10">
         	<extension>
			<m:Toolbar>
				<m:Title id="idTitle" text="{i18n>coDisplayTitle}"></m:Title>
				<m:ToolbarSpacer/>
				<m:Button icon="sap-icon://delete" press="onDelete"></m:Button>
			</m:Toolbar>
			</extension>
		<columns>
					<Column>
						<m:Label text="{i18n>coDate}" />
						<template>
							<m:Text text="" />
						</template>
					</Column>
					<Column>
						<m:Label text="{i18n>coMaterial}" />
						<template>
							<m:Text text=""/>
						</template>
					</Column>
					<Column>
						<m:Label text="{i18n>coQty}" />
						<template>
							<m:Text text="" />
						</template>
					</Column>
					<Column>
						<m:Label text="{i18n>coWeight}" />
						<template>
							<m:Text text=""/>
						</template>
					</Column>
					<Column>
						<m:Label text="{i18n>coRemarks}" />
						<template>
							<m:Text text=""/>
						</template>
					</Column>
		</columns>
	</Table>
		</m:content>
			<m:footer>
			<m:Toolbar>
				<m:ToolbarSpacer/>
				<m:Button text="{i18n>coSave}" press="onSave" type="Emphasized"/>
				<m:Button text="{i18n>coClear}" press="onClear" type="Reject"/>
				</m:Toolbar>
		</m:footer>
			</m:Page>
</core:FragmentDefinition>
