<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
<VBox class="sapUiSmallMargin">
<f:Form id="idCoForm1" editable="true">
<f:layout>
	<f:GridLayout hSpacing="1" vSpacing="1"
		labelSpanXl="4" labelSpanL="4" labelSpanM="2"
		emptySpanXL="4" emptySpanL="4" emptySpanM="0"
		columnsXL="4" columnsL="4" columnsM="10"
		singleContainerFullSize="false" >
	</f:GridLayout>
</f:layout>
<f:formContainers>
<f:FormContainer>
<f:formElements>
<f:FormElement label="{i18n>coDate}">
<f:fields>
<DatePicker id="idCoDate"
		valueFormat="dd.MM.yyyy"
		displayFormat="dd.MM.yyyy"
		value="{
		path:'coView/Date',
		formatter:'.formatter.getFormattedDate'
		}">
		<layoutData>
		<l:GridData span="XL3 L3 "/>
		</layoutData>
</DatePicker>
</f:fields>
</f:FormElement>
<f:FormElement label="{i18n>coDelDate}">
<f:fields>
<DatePicker id="idCoDelDate"
		width="50%"
		valueFormat="dd.MM.yyyy"
		displayFormat="dd.MM.yyyy"
		value="{
		parts: [{path: 'coView/DelDate'},{path: '1'}],
		formatter:'.formatter.getFormattedDate'}">
</DatePicker>
</f:fields>
</f:FormElement>
<f:FormElement label="{i18n>coCustomer}">
<f:fields>
<Input id="idCoCustomer" value="{coView>Customer}" showValueHelp="true" valueHelpRequest="onValueHelp"></Input>
<Input id="idCoCustomerText" value="" editable="false"></Input>
</f:fields>
</f:FormElement>
</f:formElements>
<f:layoutData>
<f:GridContainerData halfGrid="true"></f:GridContainerData>
</f:layoutData>
</f:FormContainer>
</f:formContainers>
</f:Form>
</VBox>
</core:FragmentDefinition>
