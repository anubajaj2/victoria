<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
<VBox class="sapUiSmallMargin">
<f:Form id="idCoForm1" editable="true">
<f:title>
<core:Title/>
</f:title>
<f:layout>
<f:ResponsiveGridLayout labelSpanXL="1" labelSpanL="1" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1" singleContainerFullSize="false"/>
</f:layout>
<f:formContainers>
<f:FormContainer>
<f:formElements>
<f:FormElement label="">
<f:fields>
<Text text="{i18n>coDate}">
	<layoutData>
	<l:GridData span="XL1 L1 M2 S4"/>
	</layoutData>
</Text>
<DatePicker id="idCoDate" class="sapUiTinyMarginTop"
		valueFormat="dd.MM.yyyy"
		displayFormat="dd.MM.yyyy"
		value="{local>/customerOrder/Date}"></DatePicker>
<layoutData>
<l:GridData span="XL3 L3 M2 S4"/>
</layoutData>
</DatePicker>
<Text text="{i18n>coDelDate}">
<layoutData>
<l:GridData span="XL1 L1 M2 S4"/>
</layoutData>
</Text>
<DatePicker id="idCoDelDate" class="sapUiTinyMarginTop"
		width="200px"
		valueFormat="dd.MM.yyyy"
		displayFormat="dd.MM.yyyy"
		value="{local>/customerOrder/DelDate}"></DatePicker>
<layoutData>
<l:GridData span="XL3 L3 M2 S4"/>
</layoutData>
</DatePicker>
</f:fields>
</f:FormElement>
<f:FormElement label="">
<f:fields>
	<Text text="{i18n>coCustomer}">
		<layoutData>
		<l:GridData span="XL1 L1 M2 S4"/>
		</layoutData>
	</Text>
<Input id="idcoCustomer" showValueHelp="true" valueHelpRequest="onValueHelp">
<layoutData>
<l:GridData span="XL3 L3 M2 S4"/>
</layoutData>
</Input>
<Input id="idcoCustomerText" value="" editable="false">
<layoutData>
<l:GridData span="XL4 L4 M2 S4"/>
</layoutData>
</Input>
</f:fields>
</f:FormElement>
</f:formElements>
</f:FormContainer>
</f:formContainers>
</f:Form>
</VBox>
</core:FragmentDefinition>
