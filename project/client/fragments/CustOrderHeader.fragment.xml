<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
<VBox class="sapUiSmallMargin">
<f:Form id="idCoForm1" editable="true">
<f:layout>
	<f:ResponsiveGridLayout
		labelSpanXl="3" labelSpanL="3" labelSpanM="2" labelSpanS="12" adjustLabelSpan="false"
		emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0"
		columnsXL="2" columnsL="2" columnsM="2"
		singleContainerFullSize="false"/>
</f:layout>
<f:formContainers>
<f:FormContainer>
<f:formElements>
<f:FormElement label="{i18n>coDate}">
<f:fields>
<DatePicker id="idCoDate"
		valueFormat="yyyy-MM-dd"
		displayFormat="dd.MM.yyyy"
		value="{path: 'local>/customerOrder/Date'}">
		<layoutData>
		<l:GridData span="XL3 L3 "/>
		</layoutData>
</DatePicker>
</f:fields>
</f:FormElement>
<f:FormElement label="{i18n>coDelDate}">
<f:fields>
<DatePicker id="idCoDelDate"
		valueFormat="yyyy-MM-dd"
		displayFormat="dd.MM.yyyy"
		value="{path: 'local>/customerOrder/DelDate'}">
		<layoutData>
		<l:GridData span="XL3 L3 "/>
		</layoutData>
</DatePicker>
</f:fields>
</f:FormElement>
<f:FormElement label="{i18n>coCustomer}">
<f:fields>
<Input id="idCoCustomer" value="{local>/coTemp/CustomerCode}" showValueHelp="true" valueHelpRequest="onValueHelp">
	<layoutData>
	<l:GridData span="XL3 L3 "/>
	</layoutData>
</Input>
<Input id="idCoCustomerText" value="" editable="false">
</Input>
</f:fields>
</f:FormElement>
</f:formElements>
<f:layoutData>
<f:GridContainerData halfGrid="true"></f:GridContainerData>
</f:layoutData>
</f:FormContainer>
</f:formContainers>
</f:Form>
</VBox>
</core:FragmentDefinition>
