<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">

  <Table items="{/Demos}" updateFinished="onUpdateFinished">
    <columns>
      <Column><Label text="Date" />
      </Column>
      <Column><Label text="Customer" />

      </Column>
      <Column><Label text="Material" />

      </Column>
    </columns>
    <items>
      <ColumnListItem>
        <cells>
          <Text text="{CrDate}" />
          <Text text="{Customer}" />
          <Text text="{Material}" />
        </cells>

      </ColumnListItem>
    </items>
  </Table>
  <Table id="idCoTable" items="">
    <toolbar>
  <m:Toolbar>
    <m:content>
      <m:Title id="idCoTitle" text="{i18n>coTableTitle}" />
      <m:ToolbarSpacer/>
      <m:Button
        icon="sap-icon://delete"
        press="onDeleteSelection"/>
    </m:content>
  </m:Toolbar>
</toolbar>
<columns>
    <Column>
      <m:Label text="{i18n>coDate}" />
    </Column>
    <Column>
      <m:Label text="{i18n>coDelivDate}" />
    </Column>
    <Column>
      <m:Label text="{i18n>coCustName}" />
    </Column>
    <Column>
      <m:Label text="{i18n>coItemName}" />
    </Column>
    <Column>
      <m:Label text="{i18n>coQty}" />
    </Column>
    <Column>
      <m:Label text="{i18n>coWeight}" />
    </Column>
    <Column>
      <m:Label text="{i18n>coMaking}" />
    </Column>
    <Column>
      <m:Label text="{i18n>coRemarks}" />
    </Column>
    <Column>
      <m:Label text="{i18n>coKarigar}" />
    </Column>
  </columns>
  <items>
    <ColumnListItem>
      <cells>
        <Text text="" />
        <Text text="" />
        <Text text="" />
        <Text text="" />
        <Text text="" />
        <Text text="" />
        <Text text="" />
        <Text text="" />
        <Text text="" />
      </cells>
    </ColumnListItem>
  </items>
  </Table>
</core:FragmentDefinition>
