<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
<Table
      id="idCoTable"
      items="{/CustomerOrders}"
      growing="true"
      mode="MultiSelect"
      updateFinished="onUpdateFinished">
  <headerToolbar>
      <Toolbar>
        <Title id="idCoTitle" text="{i18n>coTableTitle}" />
      <ToolbarSpacer/>
      <Button
        icon="sap-icon://delete"
        press="onDelete"/>
      </Toolbar>
  </headerToolbar>
<columns>
    <Column>
      <Label text="{i18n>coDate}" />
    </Column>
    <Column>
      <Label text="{i18n>coDelivDate}" />
    </Column>
    <Column>
      <Label text="{i18n>coCustName}" />
    </Column>
    <Column>
      <Label text="{i18n>coItemName}" />
    </Column>
    <Column>
      <Label text="{i18n>coQty}" />
    </Column>
    <Column>
      <Label text="{i18n>coWeight}" />
    </Column>
    <Column>
      <Label text="{i18n>coMaking}" />
    </Column>
    <Column>
      <Label text="{i18n>coRemarks}" />
    </Column>
    <Column>
      <Label text="{i18n>coKarigar}" />
    </Column>
  </columns>
  <items>
    <ColumnListItem>
      <cells>
        <Text text="{Date}" />
        <Text text="{DelDate}" />
        <Text text="{Customer}" />
        <Text text="{Material}" />
        <Text text="{Qty}" />
        <Text text="{Weight}" />
        <Text text="{Making}" />
        <Text text="{Remarks}" />
        <Text text="{Karigar}" />
      </cells>
    </ColumnListItem>
  </items>
  </Table>
</core:FragmentDefinition>
