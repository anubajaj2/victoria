<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout"
  xmlns:f="sap.ui.layout.form">
  <VBox class="sapUiSmallMargin">
  <f:Form id="idcust1" editable="true">
    <f:layout>
      <f:ResponsiveGridLayout
        labelSpanXL="2"
         labelSpanL="2"
         labelSpanM="2"
         labelSpanS="12"
         adjustLabelSpan="false"
         emptySpanXL="0"
         emptySpanL="0"
         emptySpanM="0"
         emptySpanS="0"
         columnsXL="2"
         columnsL="2"
         columnsM="2"
        singleContainerFullSize="false"/>
    </f:layout>
    <f:formContainers>
      <f:FormContainer>
    <f:formElements>
      <f:FormElement label="{i18n>Date}">
        <f:fields>
          <DatePicker id="DateId" valueFormat="yyyy-MM-dd" required="true" change="onPayDateChange"
            displayFormat="dd.MM.yyyy" value="{path: 'local>/EntryData/Date'}"></DatePicker>
          <CheckBox id="CBID" text="{i18n>contEntry}" select="onSelect" wrapping="true" selected="false">
          <layoutData>
            <l:GridData span="XL5 L5"/>
          </layoutData>
        </CheckBox>
        </f:fields>
      </f:FormElement>
      <f:FormElement label="{i18n>Customer}">
        <f:fields>
          <Input  showValueHelp="true" valueHelpOnly="true"
            valueHelpRequest="onValueHelpRequest" value="" id="idCust">
            <layoutData>
              <l:GridData span="XL3 L3"/>
            </layoutData>
          </Input>
          <Text id="idCustText" enableSelection="false"/>
        </f:fields>
      </f:FormElement>
      </f:formElements>
      <f:layoutData>
        <f:GridContainerData halfGrid="true">
        </f:GridContainerData>
      </f:layoutData>
      </f:FormContainer>
      <f:FormContainer>
        <f:formElements>
          <f:FormElement label="{i18n>tCash}">
    				<f:fields>
              <ObjectStatus id="idTC"
             class="sapUiSmallMarginBottom"
             text=""
           state="None" />
           <!-- <ObjectStatus id="idTCash"
          class="sapUiSmallMarginBottom"
          text=""
        state="Warning" /> -->

    				<!-- <Text id="idTC"/> -->
    				</f:fields>
    			</f:FormElement>
    		<f:FormElement label="{i18n>tGold}">
    			<f:fields>
            <ObjectStatus id="idG"
           class="sapUiSmallMarginBottom"
           text=""
         state="None" />
    			<!-- <Text id="idG"/> -->
    			</f:fields>
    		</f:FormElement>
    		<f:FormElement label="{i18n>tSilver}">
    			<f:fields>
            <ObjectStatus id="idS"
           class="sapUiSmallMarginBottom"
           text=""
         state="None" />
    			<!-- <Text id="idS"/> -->
    			</f:fields>
    		</f:FormElement>
        </f:formElements>
      </f:FormContainer>
        </f:formContainers>
  </f:Form>
</VBox>
</core:FragmentDefinition>
