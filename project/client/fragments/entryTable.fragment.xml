<core:FragmentDefinition xmlns="sap.ui.table" xmlns:m="sap.m" xmlns:core="sap.ui.core">
  <m:Page showHeader="false" class="sapUiContentPadding" enableScrolling="false">
		<m:content>
	<Table id="idTable" class="sapUiContentPadding" rows="" selectionMode="MultiToggle"
	visibleRowCount="10">
		<extension>
			<m:Toolbar>
				<m:Title id="idTitle" text="All Entries"></m:Title>
				<m:ToolbarSpacer/>
				<m:Button icon="sap-icon://delete" press="onDelete"></m:Button>
			</m:Toolbar>
		</extension>
		<columns>
			<Column>
				<m:Label text="{i18n>Date}"></m:Label>
				<template>
					<m:Text text=""></m:Text>
				</template>
			</Column>
			<Column>
				<m:Label text="{i18n>custcode}"></m:Label>
				<template>
					<m:Text text=""></m:Text>
				</template>
			</Column>
			<Column>
				<m:Label text="{i18n>amount}"></m:Label>
				<template>
					<m:Text text=""></m:Text>
				</template>
			</Column>
			<Column>
				<m:Label text="{i18n>Gold}"></m:Label>
				<template>
					<m:Text text=""></m:Text>
				</template>
			</Column>
			<Column>
				<m:Label text="{i18n>Silver}"></m:Label>
				<template>
					<m:Text text=""></m:Text>
				</template>
			</Column>
			<Column>
				<m:Label text="{i18n>Remarks}"></m:Label>
				<template>
					<m:Text text=""></m:Text>
				</template>
			</Column>
		</columns>
	</Table>
	</m:content>
	</m:Page>
</core:FragmentDefinition>
