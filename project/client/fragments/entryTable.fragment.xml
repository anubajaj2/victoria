<core:FragmentDefinition  xmlns="sap.m" xmlns:core="sap.ui.core">
  <!-- <Page showHeader="false" class="sapUiContentPadding" enableScrolling="false">
		<content> -->
	<Table id="idTable"
    items="{
           path: '/Entrys',
             sorter: {
             path: 'ChangedOn',
             descending: 'true'
             }
           }"
    mode="MultiSelect" growing="true"
	  growingThreshold="20" updateFinished="onUpdateFinished">
    <headerToolbar>
      <Toolbar>
        <Title id="idTitle" text="{i18n>allEntries}"></Title>
        <ToolbarSpacer/>
				<Button id = "idEntryDownload" icon="sap-icon://download" press = "onPressEntryDownload" enabled="true"/>
        <Button id="idtool" icon="sap-icon://edit" tooltip="edit entry" press="onEdit"></Button>
        <Button icon="sap-icon://delete" press="onDelete"></Button>
      </Toolbar>
    </headerToolbar>
		<columns>
			<Column>
				<Label text="{i18n>Date}"></Label>
      </Column>
      <Column>
        <Label text="{i18n>coCustName}" />
      </Column>
      <Column visible="false">
        <Label text="{i18n>custcode}"></Label>
      </Column>
      <Column visible="true">
        <Label text="{i18n>coMaterial}"></Label>
      </Column>
      <Column>
        <Label text="{i18n>amount}"></Label>

      </Column>
      <Column>
        <Label text="{i18n>Gold}"></Label>
      </Column>
        <Column>
          <Label text="{i18n>Silver}"></Label>
        </Column>
        <Column>
          <Label text="{i18n>Remarks}"></Label>
        </Column>
      </columns>
      <items>
        <ColumnListItem>
          <cells>
          <Text text= "{
                path: 'Date',
                type: 'sap.ui.model.type.Date',
                formatOptions: {
                      style: 'medium',
                      pattern: 'dd.MM.yyyy'}}"></Text>

            <Text text=""/>
            <Text text="{Customer}" />

            <Text text="{Product}" />
            <!-- <Text text="" />
            <Text text="{Material}" /> -->
					<!-- <Text text="{Customer}"></Text> -->
					<Text id="cashid" text="{path:'Cash'}"></Text>
					<Text text="{Gold}"></Text>
					<Text text="{Silver}"></Text>
					<Text text="{Remarks}"></Text>
        </cells>
      </ColumnListItem>
    </items>
	</Table>
	<!-- </content>
	</Page> -->
</core:FragmentDefinition>
