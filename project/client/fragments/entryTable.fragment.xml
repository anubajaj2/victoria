<core:FragmentDefinition  xmlns="sap.m" xmlns:core="sap.ui.core">
  <Page showHeader="false" class="sapUiContentPadding" enableScrolling="false">
		<content>
	<Table id="idTable" class="sapUiContentPadding" items="{/Entrys}" mode="MultiSelect" growing="true"
	  growingThreshold="20" updateFinished="onUpdateFinished">
    <headerToolbar>
      <Toolbar>
        <Title id="idTitle" text="{i18n>allEntries}"></Title>
        <ToolbarSpacer/>
        <Button icon="sap-icon://delete" press="onDelete"></Button>
      </Toolbar>
    </headerToolbar>
		<columns>
			<Column>
				<Label text="{i18n>Date}"></Label>
      </Column>
      <Column>
        <Label text="{i18n>custcode}"></Label>
      </Column>
      <Column>
        <Label text="{i18n>amount}"></Label>
      </Column>
      <Column>
        <Label text="{i18n>Gold}"></Label>
      </Column>
        <Column>
          <Label text="{i18n>Silver}"></Label>
        </Column>
        <Column>
          <Label text="{i18n>Remarks}"></Label>
        </Column>
      </columns>
      <items>
        <ColumnListItem>
          <cells>
          <Text text="{Date}"></Text>
					<Text text="{Customer}"></Text>
					<Text text="{Cash}"></Text>
					<Text text="{Gold}"></Text>
					<Text text="{Silver}"></Text>
					<Text text="{Remarks}"></Text>
        </cells>
      </ColumnListItem>
    </items>
	</Table>
	</content>
	</Page>
</core:FragmentDefinition>
