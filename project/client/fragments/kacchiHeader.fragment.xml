<core:FragmentDefinition
xmlns = "sap.m"
xmlns:l = "sap.ui.layout"
xmlns:f="sap.ui.layout.form"
xmlns:core ="sap.ui.core">
	<VBox class="sapUiSmallMargin">
<f:Form id="idKacchiHead"
	editable="true">
<f:toolbar>
	<Toolbar id="TB1">
		<ToolbarSpacer />
			<core:Icon
				    id="idSaveIcon"
						src="sap-icon://status-completed"
						class="size1"
						color="green"
						size="30px" alt="Save Status" tooltip="Save Status">
			</core:Icon>
			<core:Icon
						src="sap-icon://status-completed"
						class="size1"
						color="blue"
						size="30px" alt="Save Status" tooltip="Transfer Status">
			</core:Icon>
	</Toolbar>
</f:toolbar>

<f:layout>
	<f:GridLayout hSpacing="1" vSpacing="1"
		labelSpanXl="3" labelSpanL="3" labelSpanM="2"
		emptySpanXL="4" emptySpanL="4" emptySpanM="0"
		columnsXL="5" columnsL="5" columnsM="10"
		singleContainerFullSize="false" >
	</f:GridLayout>
</f:layout>
	<f:formContainers>
		<f:FormContainer>
			<f:formElements>
				<f:FormElement label="{i18n>coDate}">
					<f:fields>
            <DatePicker id="idDate" class="sapUiTinyMarginTop"
          		valueFormat="dd.MM.yyyy"
							displayFormat="dd.MM.yyyy"
							value="">
							<layoutData>
							<l:GridData span="XL3 L3"/>
							</layoutData>
          </DatePicker>
					</f:fields>
				</f:FormElement>
				<f:FormElement label="{i18n>coCustomer}">
					<f:fields>
						<Input id="idCustNo"  showValueHelp="true" valueHelpOnly="true"
									 valueHelpRequest="onCustValueHelp" liveChange="onChange"
									 value="{local>/kachhiHeaderTemp/CustomerId}">
									<layoutData>
		 							<l:GridData span="XL3 L3"/>
		 							</layoutData>
					  </Input>
						<Input id="idCustName" editable="false" value="{local>/kachhiHeaderTemp/CustomerName}">
							<layoutData>
							<l:GridData span="XL4 L5 "/>
							</layoutData>
						</Input>
					</f:fields>
        </f:FormElement>
			</f:formElements>
			<f:layoutData>
			<f:GridContainerData halfGrid="true"></f:GridContainerData>
			</f:layoutData>
		</f:FormContainer>
	</f:formContainers>
</f:Form>
</VBox>
</core:FragmentDefinition>
