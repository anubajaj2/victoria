<core:FragmentDefinition
  xmlns="sap.ui.table"
  xmlns:m="sap.m"
  xmlns:core="sap.ui.core">

  <Table id="idCustTable" rows="{/Kacchis}"
        selectionMode="MultiToggle"
				visibleRowCount="10"
				ariaLabelledBy="title">
    <toolbar>
      <m:Toolbar>
        <m:content>
          <m:Button text= "Save" type="Accept" icon="sap-icon://save" press="onSave"/>
          <m:ToolbarSpacer/>
          <m:Button icon="sap-icon://delete" press=""/>
          <m:Button icon="sap-icon://settings" press=""/>
        </m:content>
      </m:Toolbar>
    </toolbar>
  <columns>
    <Column>
      <m:Label text="{i18n>PaggaNo}" />
      <template>
        <m:Input />
      </template>
    </Column>
    <Column>
      <m:Label text="{i18n>weight}" />
      <template>
        <m:Text />
      </template>
    </Column>
    <Column>
      <m:Label text="{i18n>tunch}" />
      <template>
        <m:Text />
      </template>
    </Column>
    <Column>
      <m:Label text="{i18n>fine}" />
      <template>
        <m:Text />
      </template>
    </Column>
  </columns>
</Table>
</core:FragmentDefinition>
