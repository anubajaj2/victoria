<core:FragmentDefinition
  xmlns="sap.ui.table"
  xmlns:m="sap.m"
  xmlns:core="sap.ui.core">

  <Table id="idCustTable" rows="{kachhiLocalModel>/kachhiData}"
        selectionMode="MultiToggle"
				visibleRowCount="20"
				ariaLabelledBy="title">
        <toolbar>
        <m:Toolbar>
          <m:content>
            <m:Title id="title" text="" />
            <m:ToolbarSpacer/>
            <m:Button
              icon="sap-icon://delete"
              tooltip="Delete"
              press=""/>
            <m:Button
              icon="sap-icon://settings"
              tooltip="Settings"
              press=""/>
          </m:content>
        </m:Toolbar>
      </toolbar>
  <columns>
    <Column>
      <m:Label text="{i18n>PaggaNo}" />
      <template>
        <m:Input id="idPagga" value="{kachhiLocalModel>PaggaNo}" />
      </template>
    </Column>
    <Column>
      <m:Label text="{i18n>weight}" />
      <template>
        <m:Input id="idWeight" value="{kachhiLocalModel>Weight}" />
      </template>
    </Column>
    <Column>
      <m:Label text="{i18n>tunch}" />
      <template>
        <m:Input id="idTunch" value="{kachhiLocalModel>Tunch}"/>
      </template>
    </Column>
    <Column>
      <m:Label text="{i18n>fine}" />
      <template>
        <m:Input id="idFine" value="{kachhiLocalModel>Fine}"/>
      </template>
    </Column>
  </columns>
</Table>
</core:FragmentDefinition>
