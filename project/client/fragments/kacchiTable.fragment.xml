<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
<Table
      id="idCustTable"
      items="{kachhiLocalModel>/kachhiData}"
      growing="true"
      growingThreshold="10"
      mode="MultiSelect"
      contextualWidth="Auto"
      updateFinished="onUpdateFinished"
      inset = "true"
      keyboardMode = "Edit"
      >
  <headerToolbar>
      <Toolbar>
        <Title id="Kachhi" text="Kachhi Entries" />
      <ToolbarSpacer/>
      <Button
        icon="sap-icon://delete"
        press="onDelete"/>
      </Toolbar>
  </headerToolbar>
<columns>
    <Column hAlign="Center">
      <Label text="{i18n>PaggaNo}" />
    </Column>
    <Column hAlign="Center">
      <Label text="{i18n>weight}" />
    </Column>
    <Column hAlign="Center">
      <Label text="{i18n>tunch}" />
    </Column>
    <Column hAlign="Center">
      <Label text="{i18n>fine}" />
    </Column>
  </columns>
  <items>
    <ColumnListItem>
      <cells>
        <Input liveChange="onChange" id="idPagga"  type="Number" value="{kachhiLocalModel>PaggaNo}" editable="{= ${kachhiLocalModel>id} === '' ? true : false}"/>
        <Input id="idWeight" required="true" type="Number" value="{kachhiLocalModel>Weight}" editable="{= ${kachhiLocalModel>id} === '' ? true : false}" />
        <Input id="idTunch" required="true" type="Number" value="{kachhiLocalModel>Tunch}" editable="{= ${kachhiLocalModel>id} === '' ? true : false}"/>
        <Input id="idFine" type="Number" value="{kachhiLocalModel>Fine}" editable="{= ${kachhiLocalModel>id} === '' ? true : false}"/>
      </cells>
    </ColumnListItem>
  </items>
  </Table>
</core:FragmentDefinition>
