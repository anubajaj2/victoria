<core:FragmentDefinition xmlns:core="sap.ui.core"
  xmlns="sap.m" xmlns:t="sap.ui.table">
  <t:Table rows="{localModel>/orderItems}" visibleRowCount="20">
    <t:columns>
      <t:Column>
        <t:label>
          <Text text="Material"></Text>
        </t:label>
        <t:template>
        <Input showValueHelp="true" valueHelpRequest="onF4Help"></Input>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="Description"></Text>
        </t:label>
        <t:template>
        <Text text="{localModel>description}"></Text>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="Qty"></Text>
        </t:label>
        <t:template>
        <Input value="{localModel>qty}" maxLength="3"></Input>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="QtyD"></Text>
        </t:label>
        <t:template>
        <Input value="{localModel>qtyD}" maxLength="3"></Input>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="Weight"></Text>
        </t:label>
        <t:template>
        <Input value="{localModel>weight}" maxLength="5"></Input>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="WeightD"></Text>
        </t:label>
        <t:template>
        <Input value="{localModel>weightD}" maxLength="5"></Input>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="Making"></Text>
        </t:label>
        <t:template>
        <Input value="{localModel>making}" maxLength="5"></Input>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="MakingD"></Text>
        </t:label>
        <t:template>
        <Input value="{localModel>makingD}" maxLength="5"></Input>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="Tunch"></Text>
        </t:label>
        <t:template>
        <Text text="{localModel>tunch}"></Text>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="Remarks"></Text>
        </t:label>
        <t:template>
        <Input value="{localModel>remarks}" maxLength="80"></Input>
        </t:template>
      </t:Column>
      <t:Column>
        <t:label>
          <Text text="Sub-total"></Text>
        </t:label>
        <t:template>
        <Text text="{localModel>subTotal}"></Text>
        </t:template>
      </t:Column>
    </t:columns>
  </t:Table>
</core:FragmentDefinition>
