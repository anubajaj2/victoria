<core:FragmentDefinition
  xmlns="sap.ui.table"
  xmlns:m="sap.m"
  xmlns:core="sap.ui.core">

<Table id="OrderReturn"
  selectionMode="MultiToggle"
  rows=""
  visibleRowCount="5">
  <toolbar>
  <m:Toolbar>
    <m:Title text="{i18n>return}"></m:Title>
      <m:ToolbarSpacer></m:ToolbarSpacer>
      <m:Button icon="sap-icon://delete"
        iconFirst="true"
        text="{i18n>del}"
        tooltip="{i18n>retunTable}"
        press="onDelete">
      </m:Button>
  </m:Toolbar>
</toolbar>
 <columns>
   <Column>
     <label>
       <m:Text text="{i18n>type}"></m:Text>
     </label>
     <template>
				<m:Input></m:Input>
			</template>
   </Column>
   <Column>
     <label>
       <m:Text text="{i18n>weight}"></m:Text>
     </label>
     <template>
				<m:Input></m:Input>
			</template>
   </Column>
   <Column>
     <label>
       <m:Text text="{i18n>tunch}"></m:Text>
     </label>
     <template>
				<m:Input></m:Input>
			</template>
   </Column>
   <Column>
     <label>
       <m:Text text="{i18n>quantity}"></m:Text>
     </label>
     <template>
				<m:Input></m:Input>
			</template>
   </Column>
   <Column>
     <label>
       <m:Text text="{i18n>bhav}"></m:Text>
       </label>
     <template>
				<m:Input></m:Input>
			</template>
   </Column>
   <Column>
     <label>
       <m:Text text="{i18n>amount}"></m:Text>
     </label>
     <template>
				<m:Input></m:Input>
			</template>
   </Column>
   <Column>
     <label>
       <m:Text text="{i18n>rem}"></m:Text>
     </label>
     <template>
				<m:Input></m:Input>
			</template>
   </Column>
   <Column>
     <label>
       <m:Text text="{i18n>subTot}"></m:Text>
     </label>
     <template>
				<m:Input></m:Input>
			</template>
    </Column>
</columns>
</Table>

</core:FragmentDefinition>
