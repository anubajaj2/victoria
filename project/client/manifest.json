{
	"_version": "1.6.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "victoria",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"myService": {
				"uri": "/odata/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": ["{{keywords1}}"]
		},
		"ach": "SAP-HR"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_bluecrystal"],
		"icons": {
			"icon": "sap-icon://retail-store"
		}
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"handleValidation": true,
		"dependencies": {
			"minUI5Version": "1.44",
			"libs": {
				"sap.ui.core": {
					"minVersion": "1.44"
				},
				"sap.m": {
					"minVersion": "1.44"
				},
				"sap.ui.layout": {
					"minVersion": "1.44"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id": "mystyle"
			},
			{
				"uri": "css/googlecss.css",
				"id": "googlestyle"
			}
		]
		},
		"models": {
			"local": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "models/mockData/sampledata.json",
				"preload": true
			},
			"": {
				"dataSource": "myService",
				"preload": true
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "victoria.i18n.i18n"
				},

				"preload": true,
				"async": true
			}
		},
		"rootView": {
			"viewName": "victoria.view.MainApp",
			"type": "XML",
			"id": "idMainApp"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "victoria.view"

			},
			"routes": [
					  {
				"pattern": "split",
				"name": "App",
				"target": ["App"],
				"subroutes": [{
						"pattern": "master",
						"name": "master",
						"targetControl":"__xmlview0--idSplitApp",
						"target": ["donald"],
						"subroutes": [
							{
								"pattern": "adminPanel",
								"name": "adminPanel",

								"target": ["donald", "adminPanel"]
							},
							{
								"pattern": "downloadQueries",
								"name": "downloadQueries",
								"target": ["donald", "downloadQueries"]
							},
							{
								"pattern": "dataMaintenance",
								"name": "dataMaintenance",
								"target": ["donald", "dataMaintenance"]
							},
							{
								"pattern": "Customers",
								"name": "Customers",
								"target": ["donald", "Customers"]
							},
							{
								"pattern": "City",
								"name": "City",
								"target": ["donald", "City"]
							},
							{
								"pattern": "Group",
								"name": "Group",
								"target": ["donald", "Group"]
							},
							{
								"pattern": "Entry",
								"name": "Entry",
								"target": ["donald", "Entry"]
							},
							{
								"pattern": "Products",
								"name": "Products",
								"target": ["donald", "Products"]
							},
							{
								"pattern": "sales",
								"name": "sales",
								"target": ["donald", "sales"]
							},
							{
								"pattern": "salesws",
								"name": "salesws",
								"target": ["donald", "salesws"]
							},
							{
								"pattern": "Kacchi",
								"name": "Kacchi",
								"target": ["donald", "Kacchi"]
							},
							{
								"pattern": "Print",
								"name": "Print",
								"target": ["donald", "Print"]
							},
							{
								"pattern": "Suppliers",
								"name": "Suppliers",
								"target": ["donald", "Suppliers"]
							},
							{
								"pattern": "pendingOrders",
								"name": "pendingOrders",
								"target":["donald","pendingOrders"]
							},
							{
								"pattern": "Customizing",
								"name": "Customizing",
								"target":["donald","Customizing"]
							},
							{
								"pattern": "customerOrders",
								"name": "customerOrders",
								"target":["donald","customerOrders"]
							},
							{
								"pattern": "reports",
								"name": "reports",
								"target":["donald","reports"]
							},
							{
								"pattern": "CollaborationCenter",
								"name": "CollaborationCenter",
								"target":["donald","CollaborationCenter"]
							},
							{
								"pattern": "demo",
								"name": "demo",
								"target":["donald","demo"]
							}


						]
					}

				]
			}],
			"targets": {
				"App":{
					"viewName": "App",
					"controlId": "idMainApp",
					"controlAggregation": "pages",
					"viewLevel": 1
				},
				"donald": {
					"viewName": "View1",
					"controlId":"__xmlview0--idSplitApp",
					"controlAggregation": "masterPages",
					"viewId": "idView1",
					"viewLevel": 1
				},
				"adminPanel": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "adminPanel",
					"viewId": "idAdminPanel",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"downloadQueries": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "downloadQueries",
					"viewId": "idDownloadQueries",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"dataMaintenance": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "dataMaintenance",
					"viewId": "idDataMaintenance",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"Customers": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "Customers",
					"viewId": "idCustomers",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"City": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "City",
					"viewId": "idCity",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"Group": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "Group",
					"viewId": "idGroup",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"Entry": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "Entry",
					"viewId": "idEntry",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"Products": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "Products",
					"viewId": "idProducts",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"sales": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "sales",
					"viewId": "idsales",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"salesws": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "salesws",
					"viewId": "idsalesws",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"Kacchi": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "Kacchi",
					"viewId": "idKacchi",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"Print": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "Print",
					"viewId": "idPrint",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"Suppliers": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "Suppliers",
					"viewId": "idSuppliers",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"pendingOrders": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "pendingOrders",
					"viewId":"idpendingOrders",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"Customizing": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "Customizing",
					"viewId":"idCustomizing",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"customerOrders": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "CustomerOrders",
					"viewId":"idCustomerOrders",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"reports": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "reports",
					"viewId":"idreports",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"CollaborationCenter": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "CollaborationCenter",
					"viewId":"idcollaborationCenter",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"demo": {
					"transition": "slide",
					"controlId":"__xmlview0--idSplitApp",
					"clearAggregation": true,
					"viewName": "demo",
					"viewId":"idDemo",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				}
			}
		}
	}
}
