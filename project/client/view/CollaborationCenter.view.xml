<mvc:View controllerName="victoria.controller.CollaborationCenter"
  xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m"
  xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
  xmlns:semantic="sap.m.semantic">
  <semantic:FullscreenPage
    id="collaborationCenter"
    navButtonPress="onNavBack"
    showNavButton="false"
    title="Collaboration Center">
      <semantic:content>
      <core:Fragment fragmentName="victoria.fragments.CollaborationCenter" type="XML"></core:Fragment>
        <core:Fragment fragmentName="victoria.fragments.CollaborationCenter2" type="XML"></core:Fragment>
          <core:Fragment fragmentName="victoria.fragments.CollaborationCenter3" type="XML">
      </core:Fragment>
    </semantic:content>
    <semantic:customFooterContent>
        <Button text="{i18n>coSave}" type="Accept" press="onSave"/>
        <Button text="{i18n>coClear}" type="Reject" press="onClear"/>
    </semantic:customFooterContent>
  </semantic:FullscreenPage>
</mvc:View>
