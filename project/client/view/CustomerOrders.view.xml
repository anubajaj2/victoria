<mvc:View controllerName="victoria.controller.CustomerOrders"
  xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.m"
  xmlns:semantic="sap.m.semantic">
  <semantic:FullscreenPage
    id="customerOrders"
    navButtonPress=""
    showNavButton="true"
    title="{i18n>CustomerOrders}"
    busy="{objectView>/busy}"
    busyIndicatorDelay="{objectView>/delay}">
    <semantic:customHeaderContent>
      <!-- <Text id="idDB"> </Text> -->
      <Text id="idUser"> </Text>
      <Button icon="sap-icon://log" text="{i18n>Logout}" type="Emphasized" tooltip="{i18n>Logout}" press="logOutApp"/>
    </semantic:customHeaderContent>
    <semantic:content>
      <core:Fragment fragmentName="victoria.fragments.CustOrderHeader" type="XML">
      </core:Fragment>
    </semantic:content>
    <semantic:content>
      <core:Fragment fragmentName="victoria.fragments.CustOrderMaterial" type="XML">
      </core:Fragment>
    </semantic:content>
    <semantic:content>
      <core:Fragment fragmentName="victoria.fragments.CustOrderTable" type="XML">
      </core:Fragment>
    </semantic:content>
    <semantic:customFooterContent>
        <Button text="{i18n>Save}" id="acceptButton" type="Accept" press="onSave"/>
        <Button text="{i18n>coClear}" type="Reject" press="onClear"/>
    </semantic:customFooterContent>
  </semantic:FullscreenPage>
</mvc:View>
