<mvc:View controllerName="victoria.controller.Kacchi"
	xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns="sap.m"	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">

	<semantic:FullscreenPage id="Kacchi"
		navButtonPress="onNavBack" showNavButton="true" title="Kacchi">
	<semantic:content id="idContent">
    <VBox class="sapUiSmallMargin">
      <core:Fragment fragmentName="victoria.fragments.kacchiHeader" type="XML">
      </core:Fragment>
    </VBox>

			<VBox class="sapUiSmallMargin">
			     <core:Fragment fragmentName="victoria.fragments.kacchiTable" type="XML">
			     </core:Fragment>
		  </VBox>
      </semantic:content>

      <semantic:customFooterContent>
          <Button text="Print" type="Emphasized"  icon="sap-icon://print" press=""/>
          <Button text="Transfer" type="Accept" icon="sap-icon://shipping-status"  press=""/>
          <Button text="Clear Screen" type="Reject" icon="sap-icon://decline" press=""/>
      </semantic:customFooterContent>

	</semantic:FullscreenPage>

</mvc:View>
