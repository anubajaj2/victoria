<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="victoria.controller.View1"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page icon="sap-icon://" title="Select Option">
		<headerContent>
			<Button icon="sap-icon://arrow-right" press="onNext"></Button>
		</headerContent>
		<content>
			<!--<Button text="go to next screen" press="onNext"></Button>-->
			<List id="idFruitsList" growing="true" growingThreshold="10"
					mode="SingleSelectMaster" delete="onDelete"
			      selectionChange="onSelectItem">
				<items>
					<StandardListItem id="Customers" icon="sap-icon://customer" title="Customer" description=""></StandardListItem>
					<StandardListItem id="Products" icon="sap-icon://add-product" title="Material" description=""></StandardListItem>
					<StandardListItem id="City" icon="sap-icon://map" title="City" description=""></StandardListItem>
					<StandardListItem id="Group" icon="sap-icon://overview-chart" title="Group" description=""></StandardListItem>

					<StandardListItem id="Inventory" icon="sap-icon://collections-insight" title="Entry" description=""></StandardListItem>
					<StandardListItem id="sales" icon="sap-icon://crm-sales" title="Retail Sales" description=""></StandardListItem>
					<StandardListItem id="salesws" icon="sap-icon://my-sales-order" title="Wholesale Sales" description=""></StandardListItem>
					<StandardListItem id="Suppliers" icon="sap-icon://capital-projects" title="Booking" description=""></StandardListItem>
					<StandardListItem id="Kacchi" icon="sap-icon://crm-service-manager" title="Kacchi" description=""></StandardListItem>
					<StandardListItem id="editEntry" icon="sap-icon://signature" title="Edit Entry" description=""></StandardListItem>
					<StandardListItem id="pendingOrders" icon="sap-icon://order-status" title="Pending Orders" description=""></StandardListItem>
					<StandardListItem id="customCalculation" icon="sap-icon://simulate" title="Custom Calculation" description=""></StandardListItem>
					<StandardListItem id="customerOrders" icon="sap-icon://customer-order-entry" title="Customer Orders" description=""></StandardListItem>
					<StandardListItem visible="{= ${local>/Role} === 'Admin'}" id="dataMaintenance" icon="sap-icon://database" title="Data Maintenance" description=""></StandardListItem>
					<StandardListItem visible="{= ${local>/Role} === 'Admin'}" id="adminPanel" icon="sap-icon://key-user-settings" title="Admin Panel" description=""></StandardListItem>
					<StandardListItem visible="{= ${local>/Role} === 'Admin'}" id="analytical" icon="sap-icon://bar-chart" title="Analytical Panel" description=""></StandardListItem>
					<StandardListItem id="idcollaborationCenter" icon="sap-icon://collaborate" title="Collaboration Center" description=""></StandardListItem>
					<StandardListItem id="demo" icon="sap-icon://bar-chart" title="anubhav" description=""></StandardListItem>

				</items>
			</List>
		</content>
		<footer>
			<Toolbar></Toolbar>
		</footer>
	</Page>
</mvc:View>
