<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="victoria.controller.View1"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page icon="sap-icon://" title="{i18n>SelectOption}">
		<headerContent>
			<Button icon="sap-icon://arrow-right" press="onNext" tooltip="{i18n>Next}"></Button>
		</headerContent>
		<content>
			<!--<Button text="go to next screen" press="onNext"></Button>-->
			<List id="idFruitsList" growing="true" growingThreshold="10"
					mode="SingleSelectMaster" delete="onDelete"
			      selectionChange="onSelectItem">
				<items>
					<StandardListItem id="Customers" icon="sap-icon://customer" title="{i18n>Customer}" description=""></StandardListItem>
					<StandardListItem id="Products" icon="sap-icon://add-product" title="{i18n>Material}" description=""></StandardListItem>
					<StandardListItem id="City" icon="sap-icon://map" title="{i18n>City}" description=""></StandardListItem>
					<StandardListItem id="Group" icon="sap-icon://overview-chart" title="{i18n>Group}" description=""></StandardListItem>
					<StandardListItem id="Entry" icon="sap-icon://collections-insight" title="{i18n>Entry}" description=""></StandardListItem>
					<StandardListItem id="sales" icon="sap-icon://crm-sales" title="{i18n>RetailSales}" description=""></StandardListItem>
					<StandardListItem id="salesws" icon="sap-icon://my-sales-order" title="{i18n>WholesaleSales}" description=""></StandardListItem>
					<StandardListItem id="Suppliers" icon="sap-icon://capital-projects" title="{i18n>Booking}" description=""></StandardListItem>
					<StandardListItem id="Kacchi" icon="sap-icon://crm-service-manager" title="{i18n>Kacchi}" description=""></StandardListItem>
					<StandardListItem id="reports" icon="sap-icon://expense-report" title="{i18n>Reports}" description=""></StandardListItem>
					<!-- <StandardListItem id="pendingOrders" icon="sap-icon://order-status" title="{i18n>StockMaintenance}" description=""></StandardListItem> -->
					<StandardListItem id="Customizing" icon="sap-icon://simulate" title="{i18n>Customizing}" description=""></StandardListItem>
					<StandardListItem id="customerOrders" icon="sap-icon://customer-order-entry" title="{i18n>CustomerOrders}" description=""></StandardListItem>
					<!-- <StandardListItem id="demo" icon="sap-icon://bar-chart" title="{i18n>anubhav}" description=""></StandardListItem> -->
					<StandardListItem id="Print" icon="sap-icon://print" title="{i18n>PrintCustomizing}" description=""></StandardListItem>
					<StandardListItem id="Stock" icon="sap-icon://detail-view" title="{i18n>StockItems}" description=""></StandardListItem>
					<StandardListItem id="adminPanel" icon="sap-icon://key-user-settings" title="{i18n>AdminPanel}" description=""></StandardListItem>
					<StandardListItem id="dayBook" icon="sap-icon://product" title="{i18n>Daybookscreen}" description=""></StandardListItem>
					<StandardListItem visible="{= ${local>/Role} === 'Admin'}" id="dataMaintenance" icon="sap-icon://database" title="{i18n>DataMaintenance}" description=""></StandardListItem>
					<StandardListItem visible="{= ${local>/Role} === 'Admin'}" id="CollaborationCenter" icon="sap-icon://collaborate" title="{i18n>CollaborationCenter}" description=""></StandardListItem>
					<StandardListItem visible="{= ${local>/Role} === 'Admin'}" id="analytical" icon="sap-icon://bar-chart" title="{i18n>AnalyticalPanel}" description=""></StandardListItem>
				</items>
			</List>
		</content>
		<footer>
			<Toolbar></Toolbar>
		</footer>
	</Page>
</mvc:View>
