<mvc:View controllerName="victoria.controller.salesws" xmlns="sap.ui.commons"
	xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns:m="sap.m"	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">

	<semantic:FullscreenPage id="salesws"
		navButtonPress="onNavBack" showNavButton="true" title="Wholesale Sales Test">

		<semantic:content id="Header">
			<m:VBox class="sapUiSmallMargin"
				alignContent="sap.m.FlexAlignContent.Start">
			<core:Fragment fragmentName="victoria.fragments.orderHeader" type="XML">
			</core:Fragment>
		</m:VBox>
	</semantic:content>

	<semantic:content id="WSTable">
		<core:Fragment fragmentName="victoria.fragments.orderItems" type="XML">
		</core:Fragment>
</semantic:content>

<semantic:content id="Return">
	<m:VBox class="sapUiSmallMargin">
	<core:Fragment fragmentName="victoria.fragments.orderReturn" type="XML">
	</core:Fragment>
</m:VBox>
</semantic:content>

		<semantic:customFooterContent>
				<Button text="Print"    type="Accept" press="onPrint"/>
				<Button text="Transfer" type="Accept" press="onTransfer"/>
				<Button text="Save"     type="Reject" press="onSaveOrder"/>
				<Button text="Clear"    type="Ghost"  press="clearData"/>
		</semantic:customFooterContent>

	</semantic:FullscreenPage>

</mvc:View>
