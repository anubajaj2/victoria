<mvc:View controllerName="victoria.controller.salesws" xmlns="sap.ui.commons"
	xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns:m="sap.m"	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">

	<semantic:FullscreenPage id="salesws"
		navButtonPress="onNavBack" showNavButton="true" title="Wholesale Sales"
		class="sapUiContentPadding">

		<semantic:content id="Header">
			<m:VBox class="sapUiSmallMargin"
				alignContent="sap.m.FlexAlignContent.Start">
			<core:Fragment id="WSHeaderFragment" fragmentName="victoria.fragments.WSOrderHeader" type="XML">
			</core:Fragment>
		</m:VBox>
	</semantic:content>

	<semantic:content id="WSTable">
			<m:VBox class="sapUiSmallMargin">
		<core:Fragment id="WSItemFragment" fragmentName="victoria.fragments.orderItems" type="XML">
		</core:Fragment>
		</m:VBox>
</semantic:content>

<semantic:content id="Return">
	<m:VBox class="sapUiSmallMargin">
	<core:Fragment id="WSReturnFragment" fragmentName="victoria.fragments.orderReturn" type="XML">
	</core:Fragment>
</m:VBox>
</semantic:content>

<semantic:customFooterContent>
	<m:Button text="Save" id="idSaveButton" type="Accept" icon="sap-icon://save"  press="onSave"/>
	<m:Button text="Transfer" id="idTransferButton" type="Accept" icon="sap-icon://shipping-status"  press=""/>
	<m:Button text="Print" id="idPrintButton" type="Emphasized"  icon="sap-icon://print" press=""/>
	<m:Button text="Clear Screen" id="idClearButton" type="Reject" icon="sap-icon://decline" press="onClear"/>
</semantic:customFooterContent>

	</semantic:FullscreenPage>

</mvc:View>
