{
  "name": "StockItem",
  "plural": "StockItems",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Date": {
      "type": "date",
      "required": true
    },
    "OrderNo": {
      "type": "string",
      "required": false
    },
    "Material": {
      "type": "string",
      "required": true
    },
    "Qty": {
      "type": "number",
      "required": true,
      "default":0
    },
    "Weight": {
      "type": "number",
      "required": true,
      "default":0
    },
    "Remarks": {
      "type": "string",
      "required": false
    },
    "CreatedOn": {
			"type": "date",
			"default": "null"
		},
		"CreatedBy": {
			"type": "string",
			"default": "null"
		},
		"ChangedOn": {
			"type": "date",
			"default": "null"
		},
		"ChangedBy": {
			"type": "string",
			"default": "null"
		}
  },
  "scope":{

  },
  "validations": [],
  "relations": {
    "ToOrderHeader":{
      "type":"belongsTo",
      "model":"OrderHeader",
      "foreignKey":"OrderNo"
    },
    "ToStock":{
      "type":"hasMany",
      "model":"stockMaint",
      "foreignKey":"Material"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
